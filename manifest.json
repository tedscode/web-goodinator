{
    "name": "Web Goodinator",
    "version": "1.0",
    "description": "Bringing the best font out there to the web.",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": {
          "128": "icon.png"
        }
    },
    "permissions": ["activeTab", "tabs", "<all_urls>"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "manifest_version": 2
}
