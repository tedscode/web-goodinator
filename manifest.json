{
    "name": "Comic Injector",
    "version": "1.0",
    "description": "Bringing the best font out there to the web.",
    "icons": {
        "128": "icon.png"
    },
    "action": {
        "default_icon": {
          "128": "icon.png"
        }
    },
    "host_permissions": ["*://*/*", "<all_urls>"],
    "permissions": ["activeTab", "tabs", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "manifest_version": 3
}
